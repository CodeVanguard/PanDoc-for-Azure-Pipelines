{
  "manifestVersion": 1,
  "id": "code-vanguard-pandoc-tasks",
  "name": "PanDoc Tools",
  "version": "1.1.0",
  "publisher": "CodeVanguard",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Task for integrating PanDoc into your build and release pipelines.",
  "categories": [
    "Azure Pipelines"
  ],
  "public": true,
  "tags": [
    "Code Vanguard",
    "PanDoc",
    "Pan Doc",
    "Documentation",
    "Markdown",
    "docx"
  ],
  "icons": {
    "default": "icon.png"
  },
  "branding": {
    "color": "rgb(34, 34, 34)",
    "theme": "dark"
  },
  "screenshots": [],
  "links": {
    "home": {
      "uri": "https://github.com/CodeVanguard/PanDoc-for-Azure-Pipelines"
    },
    "support": {
      "uri": "https://github.com/CodeVanguard/PanDoc-for-Azure-Pipelines/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/CodeVanguard/PanDoc-for-Azure-Pipelines"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "license.md"
    },
    "privacypolicy": {
      "path": "privacypolicy.md"
    }
  },
  "files": [
    {
      "path": "BuildAndReleaseTasks/Tasks"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "badges": [
    {
      "href": "https://github.com/CodeVanguard/PanDoc-for-Azure-Pipelines",
      "uri": "https://dev.azure.com/CodeVanguard/PanDoc%20for%20Azure%20Pipelines/_apis/build/status/Package%20and%20Publish?branchName=main",
      "description": "Build for main branch"
    }
  ],
  "contributions": [
    {
      "id": "RunPanDoc",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "BuildAndReleaseTasks/Tasks/RunPanDoc"
      }
    }
  ]
}
